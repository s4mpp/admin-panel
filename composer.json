{
    "name": "s4mpp/admin-panel",
    "description": "Admin Panel for Laravel apps",
    "authors": [
        {
            "name": "Samuel Pacheco",
            "email": "S4mpp.dev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "S4mpp\\AdminPanel\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.10",
        "s4mpp/laraguard": "^1.0",
        "s4mpp/input": "^1.0",
        "s4mpp/element": "^1.0"
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan vendor:publish --tag=admin-assets"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "S4mpp\\AdminPanel\\Providers\\AdminPanelServiceProvider"
            ]
        }
    }
}


